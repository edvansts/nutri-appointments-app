{
  "expo": {
    "name": "Nutri Consultas +",
    "slug": "nutri-appointments",
    "orientation": "portrait",
    "version": "1.0.0",
    "icon": "./src/assets/img/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/img/logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["./src/assets"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.matosed.nutriappointments"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.matosed.nutriappointments",
      "useNextNotificationsApi": true
    },
    "extra": {
      "eas": {
        "projectId": "ba051739-8e72-4ce6-9400-43f3b04af124"
      }
    },
    "owner": "matos-ed",
    "plugins": ["expo-build-properties"]
  }
}
